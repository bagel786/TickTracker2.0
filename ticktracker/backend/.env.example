# TickTracker Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Database
DATABASE_URL=sqlite:///./ticktracker.db

# Ticketmaster API
# Get your keys at: https://developer.ticketmaster.com/
TICKETMASTER_API_KEY=your_ticketmaster_api_key_here
TICKETMASTER_SECRET=your_ticketmaster_secret_here

# Eventbrite API
# Get your keys at: https://www.eventbrite.com/platform/api
EVENTBRITE_API_KEY=your_eventbrite_api_key_here
EVENTBRITE_CLIENT_SECRET=your_eventbrite_client_secret_here
EVENTBRITE_PRIVATE_TOKEN=your_eventbrite_private_token_here
EVENTBRITE_PUBLIC_TOKEN=your_eventbrite_public_token_here

# SeatGeek API
# Get your keys at: https://platform.seatgeek.com/
SEATGEEK_CLIENT_ID=your_seatgeek_client_id_here
SEATGEEK_CLIENT_SECRET=your_seatgeek_client_secret_here

# CORS Origins (comma-separated for production)
# Example: https://yourdomain.com,https://www.yourdomain.com
CORS_ORIGINS=*
